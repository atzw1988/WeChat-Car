/**index.wxss**/
.container{
  padding: 0;
}
.swiper-tab{
    width: 100%;
    height: 44px;
    /* border-bottom: 2rpx solid #777777; */
    text-align: center;
    line-height: 40px;
    background-color: rgb(255,255,255);
    }
.swiper-tab-list{
    font-size: 16px;
    display: inline-block;
    width: 33.33%;
    color: rgb(144,144,144);
}  
.on{
    color: rgb(50,181,28);
    border-bottom: 2px solid rgb(50,181,28);
}

.show{
  display:block;
}
.hidden{
  display:none;
}

.swiper-box{
    position: relative;
    display: block;
    height: 100%;
    width: 100%;
    overflow: hidden;
}

.swiper-box view{
    text-align: center;
}

.map_container{
  height: 100%;
}

#map{
  position: relative;
}

.parking-btn{
  position: absolute;
  bottom: 31px;
  left: 50%;
  z-index: 99;
  margin-left: -200rpx; 
  width: 400rpx;
  height: 35px;
  font-size: 18px;
  line-height: 35px;
  color: rgb(255,255,255);
  background-color:  #0982d3;
  border-radius: 15px;
  box-shadow: 2px 2px 2px #5cbcfe;
  border: 1px solid #fff;
}

/*切换停车与锁  */
 .parking-s{
  position: fixed;
  top: 35px;  
  right: 14px;
  width: 80px;
  height: 80px;
  font-size: 12px;
  color: rgb(48,48,48);
  text-align: center;
  background-color: rgb(255,255,255);
}

.parking-s cover-image{
  width: 56px;
  height: 61px;
  margin: 0 auto;
}

.parking-p{
  position: fixed;
  top: 130px; 
  right: 14px;
  width: 80px;
  height: 80px;
  font-size: 12px;
  color: rgb(48,48,48);
  text-align: center;
  background-color: rgb(255,255,255);
}
.parking-p cover-image{
  width: 56px;
  height: 61px;
  margin: 0 auto;
} 

.refresh_ico,.center_ico,.help_ico,.personal_ico{
  width: 70px;
  height: 70px;
}

.refresh_ico{
  position: absolute;
  left: 14px;
  bottom: 61px;
}

.center_ico{
  position: absolute;
  left: 14px;
  bottom: 12px;
}

.help_ico{
  position: absolute;
  right: 14px;
  bottom: 61px;
}

.personal_ico{
  position: absolute;
  right: 14px;
  bottom: 12px;
}

.user_box{
  width: 100%;
  height: 180px;
  /* position: absolute; */
}

.park_box{
  /* position: fixed; */
  left: 0;
  bottom: 0;
  width: 100%;
  height: 180px;
  text-align: center;
  background-color: #fff;
  /* padding-top: 25px; */
  /* z-index: 8888; */
}
.user_name{
  font-size: 15px;
  color: #606060;
  margin: 30px 0 18px;
}
.park_info{
  margin-top: 30px;
  width: 33.3%;
  float: left;
}
.park_cont{
  font-size: 18px;
  color: #32b51c;
  margin-bottom: 5px;
}
.park_title{
  font-size: 12px;
  color: #606060;
  margin-bottom: 20px;
}
.consu_box{
  width: 100%;
  height: 35px;
}
.duration_btn{
  width: 60%;
  height: 35px;
  color: #fff;
  font-size: 14px;
  text-align: center;
  line-height: 35px;
  border-radius: 5px;
  margin: 0 auto;
  background: linear-gradient(to right, #5cbcfe , #9cd7ff);
  box-shadow: 2px 2px 2px #91d3ff;
}
.consu_btn{
  width: 35%;
  height: 35px;
  font-size: 14px;
  color: #fff;
  text-align: center;
  line-height: 35px;
  background-color: #32b51c;
  border-radius: 5px;
  margin-right: 10%;
  float: right;
}

.parking-list-container{
  margin-bottom: 0rpx;
  z-index: 999
  
}
.parking-list-section{
    background-color: white;
    /*padding: 20rpx;*/
    /*margin: 20rpx 0;*/
    display: flex;
    /*border-bottom: 1rpx solid gainsboro;*/
    border-radius: 15rpx;
    margin:10rpx 15rpx;
}
.content{
    justify-content: space-between;
    padding:10rpx;
    flex: 1
}

.parking-list-item{
    margin: 15rpx;
}

.plain-text{
    font-size: 14px;
    color: #666;
    text-align: left !important;
}

.parking-name{
    max-width: 300px;
    font-size: 16px;
    color:#222;
    overflow: hidden;  
    text-overflow: ellipsis;  
    white-space: nowrap;  
    line-height: 36rpx; 
}
.left-number{
    display: inline-block;
    color: #32b51c;
    font-size: 16px;
}

.parking-distance{
    display: inline-block;
    font-size: 14px;
    color: #32b51c;
}
.arrow-right{
  width: 11px;
  height: 17px;
}
.menu-annotation{
  text-align: center;
  margin: 20px 0;
  padding-bottom: 30px;
}
.text{
    font-size: 12px;
    position: absolute;
    bottom: 40px;
}
.nearpark{
    font-size: 14px;
    display: inline-block;
    margin-left: 10px;
}
.nearpark>text{
    color: #0982d3;
}
.allpark{
    font-size: 14px;
    display: inline-block;
    margin-left: 160px;
    color: #0982d3;
}
.choose{
    width: 80%;
    height: 200px;
    /* border: 1px solid salmon; */
    margin: 60% auto;
    background: rgba(255, 255, 255, 0.548);
    border-radius: 10px;
}
.btn{
    width: 35%;
    height: 40px;
    /* display: inline-block; */
    float: left;
    background: #5cbcfe;
    line-height: 40px;
    text-align: center;
    margin-top: 40px;
    margin-left: 10%;
    color: #fff;
    /* border: 1px solid #fff; */
}
.titletext{
    margin-top: 20px;
    font-weight: 700;
    font-size: 20px;
}
.textcontent{
    font-weight: 600;
    margin-top: 20px;
}
.close{
    font-size: 20px;
    font-weight: 700;
    float: right;
    margin-top: 10px;
    margin-right: 10px;
}
.drawer_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
}
.drawer_box {
  width: 650rpx;
  height: 320rpx;
  overflow: hidden;
  position: fixed;
  top: 70%;
  left: 0;
  z-index: 1001;
  background: #FAFAFA;
  margin: -150px 50rpx 0 50rpx;
  border-radius: 10px;
}
.drawer_title{
    text-align: center;
    font-weight: 700;
    font-size: 20px;
    margin-top: 10px;
}
.drawer_text{
    text-align: center;
    font-weight: 500;
    font-size: 18px;
    margin-top: 10px;
}
.phone_btn{
    width: 100%;
    height: 100%;
    font-size: 18px;
    line-height: 35px;
    color: rgb(255,255,255);
    background-color:  #0982d3;
}
