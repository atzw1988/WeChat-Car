/* pages/mycar/mycar.wxss */
.header{
  /* position: relative; */
  width: 340px;
  height: 100px;
  font-size: 12px;
  color: #222;
  text-align: center;
  /* line-height: 140px; */
  border: 1px solid #fff;
  border-radius: 10px;
  margin: 10px auto;
  background-color: #fff;
  box-shadow: 2px 2px 2px #f1f1f1;
}
.header>text{
    display: block;
}
.addcar{
    width: 30px;
    height: 30px;
    margin: 20px auto 0;
}
.mycar{
    width: 340px;
    height: 120px;
    background-color: #fff;
    margin: 10px auto;
    border-radius: 10px;
    box-shadow: 2px 2px 2px #f1f1f1;
}
.carnumber{
    width: 100%;
    height: 100px;
    border-bottom: 1px solid #f1f1f1;
}
.carmingxi{
    width: 300px;
    height: 50px;
    /* border: 1px solid black; */
    margin: 10px auto 0;
    line-height: 50px;
    font-size: 18px;
    font-weight: 700;
}
.del_img{
    width: 16px;
    height: 16px;
    float: right;
    margin-right: 50px;
    margin-top: 17px;
}
.del_text{
    float: right;
    font-size: 16px;
    font-weight: 500;
    margin-right: -50px;
}
.carkind{
    font-size: 14px;
    color: #2d2ddd;
    margin-left: 20px;
}.carkind1{
    font-size: 14px;
    color: #15e063;
    margin-left: 20px;
}
.title{
    font-size: 10px;
    line-height: 20px;
    margin-left: 20px;
    font-weight: 300;
}
.approve{
    text-decoration: underline;
    color: red;
}


