.item-icon{
    width: 20px;
    height: 20px;
    display: inline-block;
}
.title{
    display: inline-block;
}

.table {
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 2rem;
  align-items: center;
}
.td {
    width:40%;
    justify-content: center;
    text-align: center;
    font-size: 12px;
}
.td1 {
    width:100%;
    justify-content: center;
    text-align: center;
    font-size: 12px;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width: 40%;
  justify-content: center;
  background: #5cbcfe;
  color: #fff;
  display: flex;
  height: 2rem;
  align-items: center;
  font-size: 14px;
  font-weight: 500;
}
.th1 {
  width: 100%;
  justify-content: center;
  background: #5cbcfe;
  color: #fff;
  display: flex;
  height: 2rem;
  align-items: center;
  font-size: 14px;
  font-weight: 500;
}
.img-box{
  margin-top: 50px;
  font-size: 14px;
  color: #999;
  text-align: center;
}

.img-box > image {
  display: block;
  width: 206px;
  height: 169px;
  margin: 0 auto;
}

.img-box > text {
  margin-top: 10px;
}
.goquery{
    margin: 60px auto 0;
    width: 80%;
    color: #fff;
    font-weight: 600;
    background: linear-gradient(to right, #5cbcfe , #9cd7ff);
    box-shadow: 2px 2px 2px #5cbcfe;
}