.table {
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 2rem;
  align-items: center;
}
.td {
    width:40%;
    justify-content: center;
    text-align: center;
    font-size: 10px;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width: 40%;
  justify-content: center;
  background: #5cbcfe;
  color: #fff;
  display: flex;
  height: 2rem;
  align-items: center;
  font-size: 14px;
  font-weight: 500;
}
.gopay{
    width: 80%;
    height: 30px;
    margin: 40px auto 0;
    background-color: #5cbcfe;
    font-size: 16px;
    font-weight: 500;
    color: #fff;
    line-height: 30px;
}
.line-box{
  width: 267px;
  margin: 20px auto;
  text-align: center
}
.line-text{
  font-size: 12px;
  color: #909090;
  /* margin: 0 12px;
  float: left; */
}
.line-one{
  display: block;
  width: 88px;
  border: 1rpx #f8f8f8 solid;
  float: left;
  margin-top: 13px;
}
.line-two{
  display: block;
  width: 88px;
  border: 1rpx #f8f8f8 solid;
  float: right;
  margin-top: 13px;
}

.park_info{
  margin-top: 30px;
  width: 33.3%;
  float: left;
  height: 100px;
}
.park_info1{
  margin: 30px auto 0;
  width: 80%;
  /* float: left; */
}
.park_cont{
  font-size: 16px;
  color: #5cbcfe;
  text-align: center;
  margin-bottom: 5px;
}
.park_cont1{
  font-size: 16px;
  color: #5cbcfe;
  text-align: center;
  margin-bottom: 5px;
  margin-left: 20px;
}
.park_title{
  font-size: 16px;
  font-weight: 500;
  color: #000000;
  text-align: center;
  margin-bottom: 20px;
}

.money{
  font-size: 36px;
  color: #222;
  font-weight: bold;
  text-align: center;
  line-height: 50px;
}

.money_all{
  font-size: 14px;
  color: #909090;
  margin: 12px 0 20px 30px;
}
.share_btn{
  width: 305px;
  height: 40px;
  font-size: 18px;
  color: #fff;
  text-align: center;
  line-height: 40px;
  background-color: rgb(50, 181, 28);
  border-radius: 4px;
  margin: 50px auto 18px;
  letter-spacing: 2px;
}
.money_info{
  font-size: 12px;
  color: #909090;
  margin-left: 55px;
}
.money_remain{
  color: rgb(50, 181, 28);
}
.img-box{
  margin-top: 50px;
  font-size: 14px;
  color: #999;
  text-align: center;
}

.img-box > image {
  display: block;
  width: 206px;
  height: 169px;
  margin: 0 auto;
}

.img-box > text {
  margin-top: 10px;
}